{
  "name": "fabschurt/silex-provider-captcha",
  "description": "Captcha service provider for Silex 2.",
  "type": "library",
  "homepage": "https://github.com/fabschurt/silex-provider-captcha",
  "keywords": ["silex", "provider", "captcha"],
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Schurter",
      "email": "fabien@fabschurt.com",
      "homepage": "http://fabschurt.com/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "dev@fabschurt.com",
    "issues": "https://github.com/fabschurt/silex-provider-captcha/issues",
    "source": "https://github.com/fabschurt/silex-provider-captcha/tree/master",
    "docs": "https://github.com/fabschurt/silex-provider-captcha/blob/master/README.md"
  },
  "require": {
    "php": "^5.6|^7.0",
    "ext-gd": "*",
    "pimple/pimple": "^3.0",
    "silex/silex": "^2.0",
    "symfony/form": "^2.8|^3.0",
    "symfony/http-foundation": "^2.8|^3.0",
    "symfony/options-resolver": "^2.8|^3.0",
    "symfony/validator": "^2.8|^3.0",
    "gregwar/captcha": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.12",
    "phpunit/phpunit": "^5.6",
    "phpunit/php-invoker": "^1.1",
    "codeception/specify": "^0.4",
    "codeception/verify": "^0.3",
    "phake/phake": "~2.3",
    "symfony/browser-kit": "^2.8|^3.0",
    "symfony/config": "^2.8|^3.0",
    "symfony/css-selector": "^2.8|^3.0",
    "symfony/dom-crawler": "^2.8|^3.0",
    "symfony/translation": "^2.8|^3.0",
    "symfony/twig-bridge": "^2.8|^3.0",
    "twig/twig": "^1.27"
  },
  "autoload": {
    "psr-4": {
      "FabSchurt\\Silex\\Provider\\Captcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FabSchurt\\Silex\\Provider\\Captcha\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
